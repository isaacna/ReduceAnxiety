{
    "name": "NoAnxiety",
    "manifest_version" : 2,
    "version": "0.0.1",
    "description": "An extension to help people reduce anxiety",
    "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },

    "permissions": [
        "tabs",
        "storage",
        "topSites"
    ],
     "content_scripts": [
    {
      "matches": ["http://www.google.com/*"],
      "js": ["popup.js", "calm.js"],
      "all_frames": true
    }
  ]


}
